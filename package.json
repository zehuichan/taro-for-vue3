	},
	"devDependencies": {
		"@babel/core": "^7.23.6",
		"@tarojs/cli": "3.6.20",
		"@tarojs/webpack5-runner": "3.6.20",
		"@types/webpack-env": "^1.18.4",
		"@unocss/webpack": "^0.58.0",
		"@vue/babel-plugin-jsx": "^1.1.5",
