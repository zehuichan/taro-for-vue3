{
  "name": "taro-tpl",
  "version": "1.0.0",
  "private": true,
  "description": "taro tpl for vue3",
  "templateInfo": {
    "name": "vue3-vuex",
    "typescript": false,
    "css": "less"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "cross-env NODE_ENV=development npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "cross-env NODE_ENV=development npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "stage:weapp": "cross-env NODE_ENV=staging taro build --type weapp",
    "stage:alipay": "cross-env NODE_ENV=staging taro build --type alipay",
    "release": "release-it && git push --follow-tags origin master",
    "new": "plop"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "zehuichan",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "@nutui/nutui-taro": "^3.1.20",
    "@tarojs/components": "3.4.8",
    "@tarojs/plugin-framework-vue3": "3.4.8",
    "@tarojs/plugin-html": "3.4.8",
    "@tarojs/runtime": "3.4.8",
    "@tarojs/taro": "3.4.8",
    "dayjs": "^1.11.1",
    "pinia": "^2.0.13",
    "taro-plugin-pinia": "^1.0.0",
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@tarojs/mini-runner": "3.4.8",
    "@tarojs/webpack-runner": "3.4.8",
    "@types/webpack-env": "^1.16.4",
    "babel-preset-taro": "3.4.8",
    "cross-env": "^7.0.3",
    "eslint": "^8.14.0",
    "eslint-config-taro": "3.4.8",
    "eslint-plugin-vue": "^8.7.1",
    "release-it": "^15.0.0",
    "stylelint": "14.8.2",
    "terser-webpack-plugin": "^5.3.1",
    "vue-loader": "^17.0.0"
  },
  "release-it": {
    "npm": {
      "publish": false
    },
    "git": {
      "push": false,
      "tagName": "v${version}",
      "commitMessage": "chore: release v${version}"
    }
  }
}
