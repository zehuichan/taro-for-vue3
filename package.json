		"css-loader": "6.8.1",
		"release-it": "^17.0.1",
		"style-loader": "3.3.3",
		"unocss": "^0.58.0",
		"unocss-preset-weapp": "^0.58.0",
		"unplugin-vue-components": "^0.26.0",
		"vue-loader": "^17.3.1",
		"webpack": "^5.89.0"
	},
	"release-it": {
		"npm": {
			"publish": false
		},
		"git": {
			"push": false,
			"tagName": "v${version}",
			"commitMessage": "chore: release v${version}"
		}
	}
